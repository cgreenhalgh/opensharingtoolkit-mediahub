<%# Thing in Thing List, in column, takes Thing as data (title, description, ...) %>
<div class="thing-in-list-holder">
  <% if @iconurl? and @iconurl!='' :  %>
    <div class="thing-in-list-icon"><img src="<%= @iconurl %>"/></div>
  <% end %>
  <div class="thing-in-list-title"><%= @title %></div>
  <div class="tag-widget-holder"></div>
  <div class="thing-in-list-clear"></div>
</div>


