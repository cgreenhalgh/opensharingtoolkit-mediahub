<%# HTML5 File detail - in FileEdit %>
<% if @state == 'nofile': %>
No file
<% else if @state == 'error': %>
Error loading file
<% else: %>
<% if @state == 'loading': %>
Loading...
<% end %>
<%= @size %> bytes, <%= if not @type? or @type=='' then 'unknown mimetype' else @type %>
<% end %>
